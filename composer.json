{
    "name": "somardesignstudios/silverstripe-elastic-search",
    "description": "A silverstripe module providing search with elasticsearch",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "elasticsearch",
        "search",
        "full text search"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.2.0",
        "elasticsearch/elasticsearch": "^7.6",
        "ramsey/uuid": "^4.7",
        "symbiote/silverstripe-queuedjobs": " ^4.0 || ^5.0",
        "silverstripe/segment-field": "^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Somar\\Search\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
